{
  "name": "oriana",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/frontend",
    "apps/backend"
  ],
  "scripts": {
    "dev:frontend": "cd apps/frontend && yarn dev",
    "build:frontend": "cd apps/frontend && yarn build",
    "start:frontend": "cd apps/frontend && yarn start",
    "dev:backend": "dotenv -e apps/backend/.env -- nodemon apps/backend/src/index.ts",
    "build:backend": "tsc -p apps/backend",
    "start:backend": "node apps/backend/dist/index.js",
    "dev": "concurrently \"yarn dev:frontend\" \"yarn dev:backend\"",
    "build": "yarn build:frontend && yarn build:backend"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase": "^11.6.1",
    "firebase-admin": "^13.3.0",
    "lucide-react": "^0.503.0",
    "mongoose": "^8.13.2",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.14.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.1",
    "eslint-config-next": "15.3.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
